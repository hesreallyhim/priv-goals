# OpenAI API key
OPENAI_API_KEY=<YOUR_OPENAI_API_KEY>
# Model name
LITE_LLM_MODEL_NAME=gpt-4o
# Base URL for OpenAI-compatible API (e.g., OpenAI, Azure OpenAI, Local LLM)
# Uncomment the following line to use the OpenAI API
LITE_LLM_API_BASE_URL=https://api.openai.com/v1
# Uncomment the following line to use a local LLM instance served by Ollama
# LITE_LLM_API_BASE_URL=http://localhost:11434

# If using Google Sheets storage, uncomment and set these variables
# STORAGE_BACKEND=google_sheets
# GOOGLE_SHEETS_CREDENTIALS=config/service_account.json
# GOOGLE_SHEETS_NAME=SQUAD GOALS

# If using CSV storage (default), uncomment and set these variables
# STORAGE_BACKEND=csv
# CSV_FILE_PATH=~/.priv_goals/goals.csv
